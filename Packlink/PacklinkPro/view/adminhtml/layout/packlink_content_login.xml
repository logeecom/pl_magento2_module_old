<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title></title>
        <css src="Packlink_PacklinkPro::css/packlink.css"/>
        <link src="Packlink_PacklinkPro::js/core/AjaxService.js"/>
        <link src="Packlink_PacklinkPro::js/core/DefaultParcelController.js"/>
        <link src="Packlink_PacklinkPro::js/core/DefaultWarehouseController.js"/>
        <link src="Packlink_PacklinkPro::js/core/FooterController.js"/>
        <link src="Packlink_PacklinkPro::js/core/OrderStateMappingController.js"/>
        <link src="Packlink_PacklinkPro::js/core/PageControllerFactory.js"/>
        <link src="Packlink_PacklinkPro::js/core/ShippingMethodsController.js"/>
        <link src="Packlink_PacklinkPro::js/core/SidebarController.js"/>
        <link src="Packlink_PacklinkPro::js/core/StateController.js"/>
        <link src="Packlink_PacklinkPro::js/core/TemplateService.js"/>
        <link src="Packlink_PacklinkPro::js/core/UtilityService.js"/>
    </head>
    <body>
        <referenceContainer name="page.messages">
            <block class="Magento\Framework\View\Element\Messages" name="messages" as="messages"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Packlink\PacklinkPro\Block\Adminhtml\Content\Login"
                   name="adminhtml.packlink.login"
                   template="Packlink_PacklinkPro::login.phtml"
                   cacheable="false"
            />
        </referenceContainer>
    </body>
</page>
